{
	"name": "ozdemirburak/laravel-6-simple-cms",
	"description": "Simple Laravel 6 CMS (Content Management System) for starters.",
	"keywords": ["php", "laravel", "cms"],
	"license": "MIT",
	"type": "project",
	"require": {
        "php": "^7.2",
		"biscolab/laravel-recaptcha": "3.6.1",
		"barryvdh/laravel-elfinder": "0.4.3",
        "barryvdh/laravel-snappy": "0.4.5",
		"cviebrock/eloquent-sluggable": "6.0.0",
		"fideloper/proxy": "4.2.1",
		"h4cc/wkhtmltoimage-amd64": "0.12.4",
		"h4cc/wkhtmltopdf-amd64": "0.12.4",
		"laracasts/flash": "3.0.3",
        "laravel/framework": "6.0.0",
		"laravel/tinker": "1.0.10",
		"predis/predis": "1.1.1",
		"spatie/laravel-analytics": "3.8.0",
		"yajra/laravel-datatables-buttons": "4.7.0",
		"yajra/laravel-datatables-oracle": "9.6.0"
	},
	"require-dev": {
		"barryvdh/laravel-debugbar": "3.5.2",
		"barryvdh/laravel-ide-helper": "2.6.4",
		"doctrine/dbal": "2.9.2",
        "facade/ignition": "1.4.10",
        "fzaninotto/faker": "1.9.2",
		"laravel/browser-kit-testing": "5.1.3",
        "mockery/mockery": "1.4.2",
        "nunomaduro/collision": "3.0.1",
        "phpunit/phpunit": "8.5.13"
	},
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
	"scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan elfinder:publish"
        ]
	}
}
